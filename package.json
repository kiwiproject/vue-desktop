{
  "name": "@kiwiproject/vue-desktop",
  "version": "0.0.0",
  "private": false,
  "description": "A Vue 3 + TypeScript desktop-style window manager library (plugin-based).",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write src/",
    "lint": "eslint src",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "test:e2e": "pnpm build && pnpm --filter demo build && playwright test",
    "test:e2e:ui": "pnpm build && pnpm --filter demo build && playwright test --ui",
    "check-docs": "node ./scripts/check-docs.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepare": "husky install"
  },
  "keywords": [
    "vue",
    "desktop",
    "window",
    "plugin",
    "ui"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiwiproject/vue-desktop.git"
  },
  "bugs": {
    "url": "https://github.com/kiwiproject/vue-desktop/issues"
  },
  "homepage": "https://github.com/kiwiproject/vue-desktop#readme",
  "peerDependencies": {
    "vue": "3.5.26"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@playwright/test": "^1.57.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "12.0.2",
    "@semantic-release/npm": "13.1.3",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@tsconfig/node22": "22.0.5",
    "@vitejs/plugin-vue": "6.0.3",
    "@vitest/coverage-v8": "^4.0.17",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.8.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-vue": "10.7.0",
    "husky": "^8.0.0",
    "prettier": "3.8.0",
    "semantic-release": "25.0.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.0",
    "vite": "7.3.1",
    "vitepress": "^1.6.4",
    "vitest": "4.0.17",
    "vue": "3.5.26",
    "vue-tsc": "3.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
